{
    "debug": true,
    "accumulate_step": 1,
    "batch_size": 32,
    "eval_batch_size": 32,
    "eval_period": 1,

    "bert_cache_dir": ".cache/bert",
    "bert_dropout": 0.0,
    "bert_learning_rate": 5e-05,
    "bert_model_name": "facebook/bart-base",
    "bert_weight_decay": 1e-05,

    "decoding_method": "beam_search",
    "train_file": "data/train.jsonl",
    "dev_file": "data/dev.jsonl",
    "test_file": "data/test.jsonl",

    "SOT_weights":5,
    "max_position_embeddings":256,
    "grad_clipping": 1.0,
    "log_path": "log/default_base_nocopy_noner",
    "k":10,
    "max_epoch": 400,
    "warmup_epoch": 5,
    "weight_decay": 0.001,
    "max_length": 256,

    "num_beams":1,

    "output_path": "output/default_base_nocopy_noner",
    "use_copy": false,
    "per_declaration" : false,
    "use_prompt": true,
    "enrich_ner": false,

    "use_extra_bert": true,
    "use_gpu": true,
    "gpu_device": 0,
    "seed": 13,
    "run_name": "default_base_nocopy_noner"
}
